                    xmlns:view="clr-namespace:ExternalModManager.MVVM.View"
                    xmlns:viewModel="clr-namespace:ExternalModManager.MVVM.ViewModel">

    <DataTemplate DataType="{x:Type viewModel:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:CustomerVM}">
        <view:Customers />
    </DataTemplate>
</ResourceDictionary>