<UserControl x:Class="ExternalModManager.View.Customers"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="600"
             d:DesignWidth="622"
             Style="{StaticResource PageStyle}">
    <Grid Margin="15">
        <StackPanel>
            <TextBox Text="{Binding Search, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"></TextBox>
            
            <Label Content="{Binding OutputSearch, Mode=OneWay}" Foreground="White" FontSize="30"></Label>
            <Label Content="{Binding OutputSearchSecond, Mode=OneWay}" Foreground="White" FontSize="30"></Label>
            <Label Content="{Binding Users, Mode=OneWay}" Foreground="White" FontSize="30"></Label>
            
            <Button Content="Click" Command="{Binding GetString}" />
        </StackPanel>
    </Grid>
</UserControl>
